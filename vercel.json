{
  "version": 2,
  "buildCommand": "cd my-profile && npm run build",
  "outputDirectory": "my-profile/frontend/dist",
  "functions": {
    "my-profile/backend/api/*.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/my-profile/backend/api/index.ts" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
